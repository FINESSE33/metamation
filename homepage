    <div id="efficiency-calculator">
        <h2>Boost Your Efficiency ð¼</h2>
        <div class="inputs-section">
            <div class="input-group">
                <label for="workers">Number of Workers</label>
                <input type="number" id="workers" value="10">
                <span class="unit">Employees Performing Task</span>
            </div>
            <div class="input-group">
                <label for="salary">Annual Salary Per Worker</label>
                <input type="number" id="salary" value="50000">
                <span class="unit">Salary in $</span>
            </div>
            <div class="input-group">
                <label for="time-savings">Time Savings</label>
                <input type="number" id="time-savings" value="120">
                <span class="unit">Minutes per Day</span>
            </div>
        </div>
        <div class="divider"></div>
        <div class="output-section">
            <span id="efficiency-gains" class="efficiency-amount">$125,000</span>
            <p>Annual savings based on <span id="working-days">250</span> working days.</p>
        </div>

        <div class="cta-section">
            <a href="https://www.metamation.net/pricing" class="cta-button">explore packages</a>
        </div>
    </div>

<script>
   const workersInput = document.getElementById("workers");
const salaryInput = document.getElementById("salary");
const timeSavingsInput = document.getElementById("time-savings");
const efficiencyGainsOutput = document.getElementById("efficiency-gains");
const workingDaysOutput = document.getElementById("working-days");

function calculateEfficiencyGains() {
    const numWorkers = parseInt(workersInput.value) || 0;
    const annualSalary = parseFloat(salaryInput.value) || 0;
    const timeSavings = parseFloat(timeSavingsInput.value) || 0;

    const workingDaysPerYear = 250;
    const workingHoursPerDay = 8;
    const minutesPerHour = 60;

    const totalWorkingHoursPerYear = workingDaysPerYear * workingHoursPerDay;
    const hourlyRatePerWorker = annualSalary / totalWorkingHoursPerYear;

    const timeSavedInHoursPerDay = timeSavings / minutesPerHour;
    const timeSavedPerYearPerWorker = timeSavedInHoursPerDay * workingDaysPerYear;

    const savingsPerWorkerPerYear = hourlyRatePerWorker * timeSavedPerYearPerWorker;

    const totalGains = savingsPerWorkerPerYear * numWorkers;

    efficiencyGainsOutput.textContent = `$${totalGains.toLocaleString()}`;
}

workersInput.addEventListener("input", calculateEfficiencyGains);
salaryInput.addEventListener("input", calculateEfficiencyGains);
timeSavingsInput.addEventListener("input", calculateEfficiencyGains);

calculateEfficiencyGains();
</script>
</body>
